#include <iostream>
#include <cmath>

/*
 1.10. Задача 2.44.[1] Дадено е естествено число n (n ≥ 1). Да се напише програма, която намира броя на тези елементи от серията числа i3+13×i×n+n3, i = 1, 2, ..., n, които са кратни на 5 или на 9.
*/

int main() {

    int n;
    std::cout << "Въведете естествено число n (n >= 1): ";
    std::cin >> n;

    int count = 0; // Брояч за кратни числа

    for (int i = 1; i <= n; i++) {
        int number = i * i * i + 13 * i * n + n * n * n;

        if (number % 5 == 0 || number % 9 == 0) {
            count++;
        }
    }

    std::cout << "Броят на елементите кратни на 5 или 9 в серията е: " << count << std::endl;

    return 0;
}
